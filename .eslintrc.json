{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "new-cap": 0,
        "no-cond-assign": 0,
        "no-plusplus": 0,
        "no-await-in-loop": 0,
        "no-use-before-define": 0,
        "no-shadow": 0
    }
}
